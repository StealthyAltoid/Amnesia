public class Item {
    private String name;
    String description;
    public Item(String n, String d)
    {
        name = n;
        description = d;
    }
    
    public String getName()
    {
        return name;
    }
    public String getDescription(){
        return description;
    }
    public String toString(){
        String str = "";
        str = name + ": " + description + "\n";
        return str;
    }
}
